#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <fcntl.h>
#include <string.h>
#include <ctype.h>
#include <sys/resource.h>
#include <sys/wait.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <poll.h>
#include <signal.h>


int main(int argc, char **argv) {
  printf("\n This is a prompt. Type 'exit' to get out! \n");
  prog = stdin;


  const char* delim = "\n\t ";
    
  char* new_line = (char * ) malloc(sizeof(char)*1024);
  new_line =  fgets( new_line, 1024 ,  prog);

  while (1) //We always read in a new line.
    {
      char * tmp= NULL;
      char * new_parse = strtok_r( new_line, delim, &tmp);
      while(strcmp(new_parse,"exit") != 0){

}

    }


  fclose(prog);

  printf("\n You entered the word 'exit'. Peace out, yo!")
    return 0;
}

